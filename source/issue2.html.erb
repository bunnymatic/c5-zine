---
"title": "C5 Zine - Issue 2 - Shelter In Place"
---

<%
asset_dir = data.issue2.asset_dir
pages = data.issue2.pages
%>

<main class="zine-sheet-count-<%= (pages.length.to_f / 8).ceil.to_i %>">
  <%= partial "sheet", locals: { pages: pages[0..7], sheet: 1, asset_dir: asset_dir } %>
  <% if pages.length > 8 %>
    <%= partial "sheet", locals: { pages: pages[8..-1], sheet: 2, asset_dir: asset_dir } %>
  <% end %>
</main>
<script type="text/javascript">
 document.addEventListener("DOMContentLoaded", function() {
   document.querySelector(".nav-item__print-button").addEventListener("click", function() { window.print(); } );
 });
</script>
